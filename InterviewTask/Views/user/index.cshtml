@{
    Layout = "~/Views/Shared/_LayoutSC.cshtml";
}

<!DOCTYPE html>

@{
    ViewBag.Title = "Welcome";
}

<style>
    table, tr, td, th {
        border: 1px solid #ccc;
        padding: 10px;
        margin: 10px;
    }
</style>
<h2>Welcome to users List</h2>
<body data-ng-app="APIModule">
    <div id="tblSubs" ng-controller="APIController">
        <table>
            <tr>
                <th>UserName</th>
            </tr>
            <tbody data-ng-repeat="user in Users">
                <tr>
                    <td>{{user.Username}}</td>
                    <td>assign in group</td>
                </tr>
            </tbody>
        </table>
    </div>
</body>
